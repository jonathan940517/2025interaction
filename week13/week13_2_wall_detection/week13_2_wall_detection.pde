void setup(){
  size(600,600);
  PlayerX=300;
  PlayerY=300;
}
void draw(){
  background(150);
  for(int i=0; i<20; i++){  
    for(int j=0; j<20; j++){ 
      if(walls[i][j]==1){
        fill(255);
        rect(i*30, j*30, 30, 30);
       }
      }
     }
  if(PlayerX+dx>=0&&PlayerX+dx<=600&&PlayerY+dy>=0&&PlayerY+dy<=600&&walls[(PlayerX+dx)/30][(PlayerY+dy)/30]!=1){
        PlayerX+=dx;
        PlayerY+=dy;
  }
  ellipse(PlayerX,PlayerY,8,8);
}
void keyPressed(){
  if(keyCode==LEFT)dx=-1;
  if(keyCode==RIGHT)dx=1;
  if(keyCode==UP)dy=-1;
  if(keyCode==DOWN)dy=1;
}
void keyReleased(){
  if(keyCode==LEFT)dx=0;
  if(keyCode==RIGHT)dx=0;
  if(keyCode==UP)dy=0;
  if(keyCode==DOWN)dy=0;
}
